<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>d67f6134-0fdc-48f7-97d5-30566913c6ef</Id>
  <Version>1.0.7.0</Version>
  <ProviderName>Nathan Hammond</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Cantonese for Excel"/>
  <Description DefaultValue="Cantonese Tools for Excel"/>
  <IconUrl DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/cantonese/cantonese-for-excel/issues"/>
  <AppDomains>
    <AppDomain>https://cantonese.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="CustomFunctionsRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <AllFormFactors>
          <ExtensionPoint xsi:type="CustomFunctions">
            <Script>
              <SourceLocation resid="Functions.Script.Url"/>
            </Script>
            <Page>
              <SourceLocation resid="Functions.Page.Url"/>
            </Page>
            <Metadata>
              <SourceLocation resid="Functions.Metadata.Url"/>
            </Metadata>
            <Namespace resid="Functions.Namespace"/>
          </ExtensionPoint>
        </AllFormFactors>
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Functions.Script.Url" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/functions.js"/>
        <bt:Url id="Functions.Metadata.Url" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/functions.json"/>
        <bt:Url id="Functions.Page.Url" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/functions.html"/>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/cantonese/cantonese-for-excel"/>
        <bt:Url id="Commands.Url" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://cantonese.github.io/cantonese-for-excel/dist/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="Functions.Namespace" DefaultValue="CANTONESE"/>
        <bt:String id="GetStarted.Title" DefaultValue="Cantonese Tools Loaded!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Cantonese"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Cantonese"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Cantonese for Excel loaded successfully. You can now use romanization conversion formulas."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Show Cantonese for Excel Taskpane"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
